{
    "api": {
        "host": "127.0.0.1",
        "port": 8194
    },
    "port" : 3000,
    "expiration" : 20,
    "https": {
        "enable": true,
        "ca": "../keys/root-ca-crt.pem",
        "cert": "../keys/server-crt.pem",
        "key": "../keys/server-key.pem"
    },
    "logging": {
        "stdout": true,
        "stdoutLevel": "debug",
        "logfile": "blpapi-http.log",
        "logfileLevel": "trace",
        "reqBody": true,
        "clientDetail": true
    },
    "service": {
        "name": "blpapi-http",
        "version": "1.0.0"
    },
    "maxBodySize": 1024,
    "throttle": {
        "burst": 100,
        "rate": 50
    }
}
